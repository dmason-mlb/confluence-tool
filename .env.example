# Confluence API Configuration
# Copy this file to .env and fill in your values

# Your Confluence domain (e.g., yoursite.atlassian.net)
CONFLUENCE_DOMAIN=your-domain.atlassian.net

# Your email address
CONFLUENCE_EMAIL=your-email@example.com

# Your API token from https://id.atlassian.com/manage-profile/security/api-tokens
CONFLUENCE_API_TOKEN=your-api-token-here

# A numeric Space ID to use for testing (e.g., 123456)
# You can find this in the URL when viewing a space
CONFLUENCE_SPACE_ID=123456

# Optional: Space key (e.g., DEMO, TEST)
CONFLUENCE_SPACE_KEY=TEST